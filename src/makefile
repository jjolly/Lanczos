lanczos: main.c lanczos.h lanczos.c axpy.c copyvector.c createfullmatrix.c createsparsematrix.c createvector.c deletefullmatrix.c deletesparsematrix.c deletevector.c dotproduct.c fillfullmatrix.c fuelledia.c fuellevector.c norm.c printfilematrix.c printfullmatrix.c printsparsematrix.c printvec.c randomzahl.c readsparsematrix.c readvec.c scalevector.c sparsematrixvector.c
	gcc -g -O0 -o lanczos main.c lanczos.c axpy.c copyvector.c createfullmatrix.c createsparsematrix.c createvector.c deletefullmatrix.c deletesparsematrix.c deletevector.c dotproduct.c fillfullmatrix.c fuelledia.c fuellevector.c norm.c printfilematrix.c printfullmatrix.c printsparsematrix.c printvec.c randomzahl.c readsparsematrix.c readvec.c scalevector.c sparsematrixvector.c -lm -I.

lanczos-cuda: main.c lanczos.c readsparsematrix.c printvec.c deletesparsematrix.c createfullmatrix.c randomzahl.c printfilematrix.c deletefullmatrix.c createsparsematrix.c
	nvcc -g -O0 -o lanczos-cuda main.c lanczos.c readsparsematrix.c printvec.c deletesparsematrix.c createfullmatrix.c randomzahl.c printfilematrix.c deletefullmatrix.c createsparsematrix.c
